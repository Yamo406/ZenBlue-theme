{
	"name": "ZenBlue",
	"colors": {
		// Application colors
		//editor
		"editor.background": "#000520",
		"editor.foreground": "#ffffff",
		"editorLineNumber.foreground": "#1e4872",
		"editorLineNumber.activeForeground": "#229bff",
		"editorBracketMatch.border": "#ffe4c279",
		"titleBar.activeBackground": "#010A3C",
		"titleBar.activeForeground": "#ffffff",

		//editor selection and highlights
		"editor.lineHighlightBackground": "#0e3672a8",
		"editor.findRangeHighlightBackground": "#0e3672a8",
		"editor.findMatchHighlightBackground": "#0e3672a8",
		"editor.findMatchBackground": "#edf9b144",
		"editor.selectionBackground": "#0e3672a8",
		"editor.selectionHighlightBackground": "#0e3672a8",
		"editor.wordHighlightBackground": "#0e3672a8",

		//list/trees
		"list.hoverBackground": "#0e3672a8",
		"list.activeSelectionBackground": "#007ce252",
		"list.activeSelectionForeground": "#ffffff",
		"list.inactiveFocusBackground": "#0e3672a8",
		"list.inactiveSelectionBackground": "#007ce252",

		//widget
		"editorWidget.background": "#010a3c",
		"editorWidget.foreground": "#ffffff",

		//input fields
		"input.background": "#000520",
		"input.foreground": "#ffffff",

		//button
		"button.background": "#007ce2",
		"button.foreground": "#ffffff",

		//dropdown
		"dropdown.background": "#010a3c",
		"dropdown.listBackground": "#010a3c",
		"dropdown.foreground": "#ffffff",

		//icon
		"icon.foreground": "#ffffff",

		//editor group
		"editorGroupHeader.tabsBackground": "#01072b",
		"editorGroupHeader.noTabsBackground": "#01072b",
		"sideBySideEditor.verticalBorder": "#007ce2",
		"sideBySideEditor.horizontalBorder": "#007ce2",

		//tabs
		"tab.activeBackground": "#000520",
		"tab.activeBorderTop": "#007ce2",
		"tab.inactiveBackground": "#010a3c",

		//panel
		"panel.background": "#000520",
		"panelTitle.activeBorder": "#007ce2",

		//debugger
		"debugToolBar.background": "#010a3c",

		//terminal
		"terminal.foreground": "#ffffff",

		//badge
		"badge.background": "#007ce2",
		"badge.foreground": "#ffffff",
		"profileBadge.background": "#007ce2",
		"profileBadge.foreground": "#ffffff",

		// cursor
		"editorCursor.background": "#000000",
		"editorCursor.foreground": "#FFE4C2",
		"terminalCursor.background": "#000000",
		"terminalCursor.foreground": "#FFE4C2",

		// sideBar
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#00031a",
		"sideBarSectionHeader.background": "#010a3c",
		"sideBarSectionHeader.foreground": "#ffffff",

		//activityBar
		"activityBar.background": "#01072b",
		"activityBar.activeBorder": "#007ce2",
		"activityBar.foreground": "#ffffff",
		"activityBarBadge.background": "#007ce2",
		"activityBarBadge.foreground": "#ffffff",

		//statusBar
		"statusBar.background": "#010a3c",
		"statusBar.noFolderBackground": "#010a3c",
		"statusBarItem.remoteBackground": "#007ce2",
		"statusBarItem.remoteForeground": "#ffffff"
	},
	// Tokent Colors (syntax)
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6289bd"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#EDF9B1"
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#438eff"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#006EFF"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#2EC0FF"
			}
		},
		{
			"name": "html tags",
			"scope": [
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#006EFF"
			}
		},
		{
			"name": "html entity name",
			"scope": ["entity.name.tag.html"],
			"settings": {
				"foreground": "#EDF9B1"
			}
		},
    {
      "name": "typescript punctuation",
      "scope": ["punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
      ],
      "settings": {
        "foreground": "#006EFF"
      }
    },
    {
      "name": "typescript tags",
      "scope": [
        "entity.name.tag.tsx",
        "meta.tag.tsx",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#EDF9B1"
      }
    },
    { "name": "Typescript object literals",
      "scope": [
        "meta.object.member.ts",
        "meta.objectliteral.ts",
        "new.expr.ts",
        "meta.var.expr.ts",
        "meta.boject-literal.key.ts"
      ],
      "settings": {
        "foreground": "#2EC0FF",
      }
    },
    {
      "name": "Python Strings ",
      "scope": [
        "string.quoted.single.python",
        "meta.function-call.arguments.python",
        "meta.function-call.python",
        "source.python"
      ],
      "settings": {
        "foreground": "#EDF9B1",
      }
    },
		{
			"name": "yaml entity name",
			"scope": ["entity.name.type.anchor.yaml"],
			"settings": {
				"foreground": "#EDF9B1"
			}
		},
		{
			"name": "php punctuation variable",
			"scope": ["punctuation.definition.variable.php"],
			"settings": {
				"foreground": "#00EEFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#2EC0FF"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00eeff"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#EDF9B1"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#00EEFF"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#2EC0FF"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#006EFF"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#EDF9B1"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#438eff"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#438eff"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": ["entity.name.method.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00eeff"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#00eeff"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#00EEFF"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": {
				"foreground": "#006EFF"
			}
		},
		{
			"name": "CSS ID's",
			"scope": ["source.sass keyword.control"],
			"settings": {
				"foreground": "#00eeff"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#438eff"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#2ec0ff"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#2ec0ff"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#2ec0ff"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00eeff"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#438eff"
			}
		},
    {
      "name": "Javascript object literals",
      "scope": [
        "meta.object.member.js",
        "meta.objectliteral.js",
        "new.expr.js",
        "meta.var.expr.js",
        "meta.boject-literal.key.js"
      ],
      "settings": {
        "foreground": "#2EC0FF",
      }
    },
		{
